   "source": [
    "\n",
    "def swap(list, pos1, pos2):\n",
    "\n",
    "\tget = list[pos1], list[pos2]\n",
    "\t\n",
    "\t\n",
    "\tlist[pos2], list[pos1] = get\n",
    "\t\n",
    "\treturn list\n",
    "\n",
    "\n",
    "List = [1,3,5,4,0]\n",
    "\n",
    "pos1, pos2 = 1, 3\n",
    "print(swap(List, pos1-1, pos2-1))\n"
   ]
