   "source": [
    "f=0\n",
    "str1=input(\"Enter the string 1\")\n",
    "str2=(\"Hi\")\n",
    "for i in range(len(str1)):\n",
    "    for j in range(len(str2)):\n",
    "        if str1[i]==str2[j]:\n",
    "         j=j+1\n",
    "    if j==len(str2):\n",
    "        f=f+1\n",
    "\n",
    "\n",
    "print(\"Substring frequence is :\",f)\n",
